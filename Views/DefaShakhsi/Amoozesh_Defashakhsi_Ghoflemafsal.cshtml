@model IEnumerable<hanmudo.Models.SelfDefence>
@{
     ViewData["description"]="آموزش پیشرفته ترین فنون دفاع شخصی وقفل مفصل در شیراز";
    ViewData["Title"] = "آموزش دفاع شخصی وقفل مفصل در شیراز";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="main_container">
    <div class="top_content">
        <span class="d-md-none menu_icon mt-2 me-2" type="button" data-bs-toggle="offcanvas" data-bs-target="#demo">
            <i class="fa fa-bars fa-2x" aria-hidden="true"></i>
        </span>

    </div>
</div>
<h1 style="font-family: shabnam;font-size: 1.2rem;font-weight: bold;text-align: center;margin-bottom: 2rem;">آموزش‌های دفاع شخصی و قفل مفصل</h1>
@if (Model.Count()==0)
{
    <div id="selfdefence_div">
        <h3>محتوای این صفحه به زودی تکمیل می‌گردد</h3>
    </div>
       <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
}
@foreach (var item in Model)
{
    <div id="selfdefence_div">
        <h3>@item.Title</h3>
        <div>
            @Html.Raw(item.Video)
        </div>
        <p>@item.Description</p>
    </div>
}
