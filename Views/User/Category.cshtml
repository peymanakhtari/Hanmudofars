@model List<hanmudo.Models.CategoryTeknik>
@{
    ViewData["Title"] = ViewBag.categoryName;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<partial name="~/Views/Shared/UserPanelTopBar.cshtml" />

<div class="teknik">
    <h3 style="font-family: shabnam;text-align: center;font-weight: bold;font-size: 1.2rem;"> @ViewBag.categoryName</h3>
     <br><br>
    @if (Model.Count==0)
    {
        <p>تکنیک های این بخش به زودی تکمیل میگردد</p>
    }
    <button style="font-family: iraniansans;margin-right:2.2rem ;" class="btn btn-dark " onclick="history.back()">بازگشت</button>
    @foreach (var item in Model)
    {
        <a href="/User/Content/@item.Id">@item.Text</a>

    }
</div>


